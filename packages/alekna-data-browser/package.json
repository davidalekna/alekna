{
  "name": "@alekna/react-data-browser",
  "version": "1.1.3",
  "description": "DataBrowser is a lightweight, fast and extendable render props component",
  "author": "David Alekna <alekna@live.co.uk>",
  "license": "MIT",
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.esm.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/*.d.ts",
    "lib/bundle*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davidalekna/alekna.git"
  },
  "bugs": {
    "url": "https://github.com/davidalekna/alekna/issues"
  },
  "homepage": "https://github.com/davidalekna/alekna#readme",
  "keywords": [
    "react",
    "table",
    "flexbox",
    "datagrid",
    "datatable",
    "file table",
    "list browser",
    "file browser",
    "data browser",
    "data grid",
    "react grid"
  ],
  "scripts": {
    "prepare": "npm run build",
    "dev": "yarn storybook",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "yarn clean && tsc -b .",
    "postbuild": "npm run bundle",
    "bundle": "cross-env-shell \"../../node_modules/.bin/rollup\" -c rollup.config.js",
    "clean": "rimraf lib storybook-static esm",
    "report-coverage": "codecov",
    "storybook": "start-storybook -p 6008",
    "storybook:build": "build-storybook",
    "storybook:upload": "gh-pages -d storybook-static",
    "storybook:clean": "rimraf storybook-static"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
