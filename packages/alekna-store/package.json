  "license": "MIT",
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.esm.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/bundle*",
    "lib/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidalekna/alekna.git"
  },
  "bugs": {
