    "storybook:upload": "gh-pages -d storybook-static",
    "storybook:clean": "rimraf storybook-static"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@storybook/react": "^5.0.11",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.132",
    "@types/react": "^16.8.14",
    "@types/styled-components": "^4.1.14",
    "@types/uuid": "^3.4.4",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
