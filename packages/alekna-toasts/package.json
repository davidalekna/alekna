{
  "name": "@alekna/react-toasts",
  "version": "1.0.2",
  "description": "Simple toasts for react.",
  "author": "David Alekna <alekna@live.co.uk>",
  "license": "MIT",
  "main": "./lib/bundle.cjs.js",
  "module": "./lib/bundle.esm.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/bundle*",
    "lib/index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidalekna/alekna.git"
  },
  "bugs": {
    "url": "https://github.com/davidalekna/alekna/issues"
  },
  "homepage": "https://github.com/davidalekna/alekna#readme",
  "keywords": [
    "react",
    "toasts"
  ],
  "scripts": {
    "prepare": "npm run build",
    "dev": "yarn storybook",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "yarn clean && tsc -b .",
    "postbuild": "npm run bundle",
    "bundle": "../../node_modules/rollup/dist/bin/rollup -c rollup.config.js",
    "clean": "rimraf lib storybook-static esm",
    "report-coverage": "codecov",
    "storybook": "start-storybook -p 6008",
    "storybook:build": "build-storybook",
    "storybook:upload": "gh-pages -d storybook-static",
    "storybook:clean": "rimraf storybook-static"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@alekna/react-store": "file:../alekna-store",
    "lodash": "^4.17.11",
    "rxjs": "^6.5.1",
    "styled-components": "^4.3.1",
    "uuid": "^3.3.2"
  },
  "directories": {
    "lib": "lib"
  }
}
